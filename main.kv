<botizao_infor>:
    name: "tela_botizao"
    font_name: 'Roboto'
    font_size: 60
    markup: True
    size_hint: .7, .5
    md_bg_color: 0.06, 0.10, 0.14
    line_color: .61, 0.68, 0.69
    pos_hint: {'center_x': .5, 'center_y': .5}
    MDBoxLayout:
        #md_bg_color: 0.06, 0.10, 0.14
        orientation: "vertical"
        md_bg_color: 0.06, 0.10, 0.14
        line_color: .61, 0.68, 0.69
        spacing: 8

        MDIconButton:
            theme_text_color: 'Custom'
            icon: 'close'
            text_color: 1, 1, 1, 1
            on_release: root.desativar_caixa_bot()

        MDCard:
            size_hint_y: .1
            orientation: "vertical"
            pos_hint: {"center_x": .5, "center_y": .45}
            size_hint_y: .1
            padding: [25, 0, 25, 0]
            md_bg_color: 0.06, 0.10, 0.14
            line_color_normal: (0,1,0,1)


            MDBoxLayout:
                halign: "center"
                orientation: "vertical"
                spacing:6

                MDLabel:
                    pos_hint: {'center_x': 0.50, 'center_y': 0.78}
                    width: 100
                    font_size: "12sp"
                    mode: 'rectangle'

                MDTextField:
                    pos_hint: {'center_x': 0.50, 'center_y': 0.72}
                    id: chat_id_input
                    width: 100
                    font_size: "12sp"
                    hint_text: 'USAR ID (GRUPO):'
                    mode: 'rectangle'
                    color: (0,1,1,1)
                    line_color_normal: (0,1,0,1)
                    text_color: (.95,.44,.49,1)

                MDTextField:
                    pos_hint: {'center_x': 0.50, 'center_y': 0.67}
                    id: api_key_input
                    width: 100
                    font_size: "12sp"
                    hint_text: 'CHAVE BOT (KEY):'
                    mode: 'rectangle'
                    color: (0,1,1,1)
                    line_color_normal: (0,1,0,1)
                    text_color: (.95,.44,.49,1)

                MDLabel:
                    pos_hint: {'center_x': 0.50, 'center_y': 0.65}
                    width: 100
                    font_size: "12sp"
                    mode: 'rectangle'


                ButtonFocus:
                    pos_hint: {'center_x': .5, 'center_y': .35}
                    size_hint_x: .9
                    text: 'INICIAR BOT!'
                    md_bg_color: 0.95, 0.17, 0.30
                    text_color: (255,255,255)
                    line_color: (0.95, 0.17, 0.30)
                    bold: True
                    on_press: root.USAR_BOT()
                MDLabel:
                    pos_hint: {'center_x': 0.50, 'center_y': 0.32}
                    width: 100
                    font_size: "12sp"
                    mode: 'rectangle'

<error404>:
    name: "error404net"
    font_name: 'Roboto'
    font_size: 60
    markup: True

    ScrollView:
        line_color: (.95,.44,.49,1)
        size: self.size
        pos: self.pos


        MDFloatLayout:
            line_color: (0.95, 0.17, 0.30)
            md_bg_color: 0.06, 0.10, 0.14
            orientation: "vertical"




            MDLabel:
                text: "_____________________ ATENÇÃO ______________________"
                halign: "center"
                font_size: "16sp"
                bold: True
                pos_hint: {'center_x': 0.50, 'center_y': 0.75}

            MDLabel:
                font_size: "12sp"
                bold: True
                halign: "center"
                pos_hint: {'center_x': 0.50, 'center_y': 0.65}
                text: "Não foi possivel conectar-se a internet, verifique se sua rede esta ativa. Essa limitação afeta o uso do sistema de analises."



